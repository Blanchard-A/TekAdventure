#include "adventure.h"
#include <stdio.h>

void	check_click(t_data *data)
{
  if ((data->mouse->x > 42 && data->mouse->x < 207) &&
      (data->mouse->y > 733 && data->mouse->y < 807))
    {
      printf("UTILISER\n");
    }
  
  
  
}
